{
  "name": "anilogin",
  "version": "0.0.1",
  "description": "Anime login system\r #Project Information\r ##Goal\r Create a node package that will have all anime provider login systems integrated.",
  "main": "index.js",
  "scripts": {
    "test1": "./node_modules/.bin/mocha --require ./start.js",
    "test": "./node_modules/.bin/mocha -r blanket --require ./start.js -R --reporter mocha-lcov-reporter |  ./node_modules/codacy-coverage/bin/codacy-coverage.js",
    "env": "$env:DEBUG =\"anilogin:*,myanimelist:*\"",
    "compile": "babel -d lib/ src/",
    "prepublish": "npm run compile",
    "blanket": {
      "pattern": "src",
      "data-cover-never": "node_modules"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bote795/AniLogin.git"
  },
  "keywords": [
    "anime",
    "anilist",
    "myanimelist",
    "humming",
    "bird"
  ],
  "author": "Nicolas Botello",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bote795/AniLogin/issues"
  },
  "homepage": "https://github.com/bote795/AniLogin#readme",
  "dependencies": {
    "debug": "^2.2.0",
    "isomorphic-fetch": "^2.2.1",
    "popura": "^1.2.4",
    "query-string": "^4.2.3"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.23.0",
    "blanket": "^1.2.3",
    "chai": "^3.5.0",
    "codacy-coverage": "^2.0.1",
    "dotenv": "^4.0.0",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.3.0"
  }
}
